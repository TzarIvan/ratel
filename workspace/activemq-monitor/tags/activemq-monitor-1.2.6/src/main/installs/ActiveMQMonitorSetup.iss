[Setup]
; NOTE: The value of AppId uniquely identifies this application.
; Do not use the same AppId value in installers for other applications.
; (To generate a new GUID, click Tools | Generate GUID inside the IDE.)
AppId={{372FC683-F91A-4FDE-9A28-6133D60125BA}
AppName=ActiveMQ Monitor
AppVerName=ActiveMQ Monitor 1.2.3
AppPublisher=Malbec Partners
AppPublisherURL=http://www.malbecpartners.com
AppSupportURL=http://www.malbecpartners.com
AppUpdatesURL=http://www.malbecpartners.com
DefaultDirName={pf}\ActiveMQ Monitor
DefaultGroupName=ActiveMQ Monitor
OutputBaseFilename=ActiveMQMonitorSetup
Compression=lzma
SolidCompression=yes

[Languages]
