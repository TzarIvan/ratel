Source: "lib\*"; DestDir: "{app}\lib"; Flags: ignoreversion recursesubdirs createallsubdirs
; NOTE: Don't use "Flags: ignoreversion" on any shared system files

[Icons]
Name: "{group}\ActiveMQ Monitor"; Filename: "%JAVA_HOME%\bin\javaw"; Parameters: """-jar"" ""{app}\activemq-monitor-1.2.3.jar"""
Name: "{group}\{cm:UninstallProgram,ActiveMQ Monitor}"; Filename: "{uninstallexe}"
Name: "{commondesktop}\ActiveMQ Monitor"; Filename: "%JAVA_HOME%\bin\javaw";  Parameters: """-jar"" ""{app}\activemq-monitor-1.2.3.jar"""; Tasks: desktopicon
